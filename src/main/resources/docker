docker run --detach
--env MYSQL_ROOT_PASSWORD=root
--env MYSQL_USER=root
--env MYSQL_PASSWORD=root
--env MYSQL_DATABASE=socialdb
--name mysql
--publish 3306:3306
mysql:8-oracle

